{
  "manifest_version": 3,
  "name": "Ryunix DevTools",
  "version": "1.2.3-canary.1",
  "description": "Ryunix DevTools Extension",
  "devtools_page": "devtools.html",
  "icons": {},
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content-script.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["hook.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
