{
  "name": "@unsetsoft/ryunixjs",
  "version": "0.4.13-nightly.5",
  "license": "MIT",
  "main": "./dist/Ryunix.js",
  "types": "./dist/Ryunix.d.ts",
  "private": false,
  "bugs": {
    "url": "https://github.com/UnSetSoft/Ryunixjs/issues"
  },
  "homepage": "https://github.com/UnSetSoft/Ryunixjs#readme",
  "scripts": {
    "build:ts": "tslint --project . --fix --force && tsc",
    "build:js": "rollup ./src/main.js --file ./dist/cj/Ryunix.js --format umd --name Ryunix",
    "prepublishOnly": "npm run build:ts",
    "postinstall": "npm run build:ts",
    "nightly:release": "npm publish --tag nightly",
    "release": "npm publish"
  },
  "dependencies": {
    "rollup": "4.9.2",
    "ts-node": "10.9.2",
    "tsup": "8.0.1",
    "typescript": "5.3.3",
    "tslint": "6.1.3"
  },
  "engines": {
    "node": ">=18.16.0"
  },
  "keywords": [
    "ryunixjs"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "tsup": {
    "entry": [
      "src/main.js"
    ],
    "format": [
      "cjs",
      "esm"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true
  },
  "files": [
    "dist"
  ]
}
