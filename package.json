{
  "name": "RyunixJS",
  "version": "1.2.0-canary.28",
  "private": true,
  "homepage": "./",
  "workspaces": [
    "packages/*"
  ],
  "license": "MIT",
  "scripts": {
    "lint-eslint": "eslint . --max-warnings=0 --config eslint.config.mjs  ",
    "lint-fix": "yarn prettier-fix && eslint . --fix --max-warnings=0 --config eslint.config.mjs  ",
    "prettier-check": "prettier -c .",
    "prettier-fix": "prettier -w .",
    "kg:init": "gmvu init",
    "kg:bump": "gmvu bump",
    "r:c": "yarn log --prerelease canary && yarn kg:bump && yarn prettier-fix && npm run canary:release --workspace=@unsetsoft/ryunix-webpack --workspace=@unsetsoft/ryunixjs && yarn git canary",
    "r:r": "yarn log main && yarn kg:bump && yarn prettier-fix && npm run release --workspace=@unsetsoft/ryunix-webpack --workspace=@unsetsoft/ryunixjs && yarn git main",
    "cra:r": "npm run release --workspace=@unsetsoft/cra",
    "cra:n": "npm run nightly:release --workspace=@unsetsoft/cra",
    "log": "standard-version --header '# Changelog'",
    "git": "git push --follow-tags origin",
    "ryunixjs:build": "yarn build:test --workspace=@unsetsoft/ryunixjs",
    "build": "yarn lint-fix && yarn build:test"
  },
  "dependencies": {
    "@babel/cli": "7.27.2",
    "@babel/core": "7.27.4",
    "@babel/eslint-parser": "7.27.5",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/plugin-transform-react-jsx": "7.27.1",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.29.0",
    "@kagarisoft/gmvu-cli": "1.0.5",
    "@swc/core": "1.12.6",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "babel-loader": "10.0.0",
    "chalk": "5.4.1",
    "command-exists-promise": "2.0.2",
    "copy-webpack-plugin": "13.0.0",
    "create-create-app": "7.3.0",
    "css-loader": "7.1.2",
    "css-minimizer-webpack-plugin": "7.0.2",
    "dotenv-webpack": "8.1.0",
    "eslint": "9.29.0",
    "eslint-plugin-eslint-plugin": "6.5.0",
    "eslint-plugin-import": "2.32.0",
    "file-loader": "6.2.0",
    "glob": "11.0.3",
    "globals": "16.2.0",
    "html-loader": "5.1.0",
    "html-webpack-plugin": "5.6.3",
    "image-webpack-loader": "8.1.0",
    "lodash": "4.17.21",
    "mini-css-extract-plugin": "2.9.2",
    "prettier": "3.6.0",
    "rollup": "4.44.0",
    "sass": "1.89.2",
    "sass-loader": "16.0.5",
    "style-loader": "4.0.0",
    "terminal-log": "1.0.1",
    "terser-webpack-plugin": "5.3.14",
    "thread-loader": "4.0.4",
    "url-loader": "4.1.1",
    "webpack": "5.99.9",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2",
    "yargs": "18.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.27.2",
    "@babel/preset-react": "7.27.1",
    "@eslint/compat": "1.3.0",
    "@rollup/plugin-terser": "0.4.4",
    "globals": "16.2.0",
    "jest": "30.0.2",
    "jest-environment-jsdom": "30.0.2",
    "standard-version": "9.5.0",
    "typescript": "5.8.3"
  },
  "packageManager": "yarn@4.9.1"
}
