{
  "name": "RyunixJS",
  "version": "1.2.3-canary.0",
  "private": true,
  "homepage": "./",
  "workspaces": [
    "packages/*",
    "test/*"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "scripts": {
    "dev": "pnpm --parallel run dev",
    "build": "pnpm --stream run build",
    "lint": "eslint . --max-warnings=0 --config eslint.config.mjs",
    "lint:fix": "pnpm run format && eslint . --fix --max-warnings=0 --config eslint.config.mjs",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "test": "pnpm -r run test",
    "clean": "pnpm -r exec rimraf node_modules && rimraf node_modules",
    "kg:init": "gmvu init",
    "kg:bump": "gmvu bump",
    "release:canary": "npm run kg:bump && pnpm run format && pnpm --parallel --filter @unsetsoft/ryunixjs run canary:release",
    "release:stable": "npm run kg:bump && pnpm run format && pnpm --parallel --filter @unsetsoft/ryunixjs run release",
    "cra:release": "pnpm run release --filter @unsetsoft/cra",
    "cra:nightly": "pnpm run nightly:release --filter @unsetsoft/cra",
    "changelog": "standard-version --header '# Changelog'",
    "git:push": "git push --follow-tags origin",
    "run:web": "pnpm --parallel --filter webpack-test run dev",
    "run:vite": "pnpm --filter ./test/web/Vite run dev",
    "run:rspack": "pnpm --filter ./test/web/Rspack run dev"
  },
  "devDependencies": {
    "@babel/preset-env": "7.27.2",
    "@babel/preset-react": "7.27.1",
    "@eslint/compat": "1.3.0",
    "@kagarisoft/gmvu-cli": "1.0.5",
    "@rollup/plugin-terser": "0.4.4",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "eslint": "9.31.0",
    "eslint-plugin-eslint-plugin": "6.5.0",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-react": "7.37.5",
    "jest": "30.0.2",
    "jest-environment-jsdom": "30.0.2",
    "prettier": "3.6.0",
    "rimraf": "5.0.5",
    "standard-version": "9.5.0",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@10.20.0"
}
